<!DOCTYPE html>
<html>
  <head>
    <title>All Bookings</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <table>
      <% for (var booking of bookings) { %>
        <tr>
            <td><%= booking.email %></td>
            <td><%= booking.numTickets %></td>
        </tr>
    <% } %>
  
         
  
    </table>
    <form class="container my-4" method="POST" action="/bookings/delete/<%= booking._id %>">
      <button type="delete" class="btn btn-primary">delete</button>
    </form>
  
   
        <% for (let i = 1; i <= Math.ceil(total / perPage); i++) { %>
            <% if(i === page) { %>
              <span><%= i %></span>
            <% } else { %>
              <a href="/bookings/paginate?page=<%= i %>&perPage=<%= perPage %>"><%= i %></a>
            <% } %>
          <% } %>
     
  </body>
</html>